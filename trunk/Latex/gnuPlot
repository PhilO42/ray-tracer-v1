set terminal png
set output "KDTimes.png"
set title "Evaluation der Laufzeit"
set xlabel "Anzahl Dreiecke"
set ylabel "Zeit in Sekunden"
set xtics 1920
set ytics (7,19,28,31)
plot "times.txt" using 1:3 with lines notitle lw 2 lc rgb "green",\
     "times.txt" using 1:3 with points notitle pt 13 ps 2
replot
