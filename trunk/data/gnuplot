set terminal png
set xrange [0:1]
set yrange [0:1]
set size ratio -1

set ylabel "Random Sampling"
set output "random.png"
set multiplot
plot "RandomSamples.txt" using 1:2 pt 7 ps 2 title ''
plot "RandomSamples.txt" using 1:3 pt 1 ps 3 title ''
plot "RandomSamples.txt" using 3:2 pt 1 ps 3 title ''
replot
unset multiplot

set output "random2.png"
set multiplot
